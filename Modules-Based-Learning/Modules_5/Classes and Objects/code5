class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def display_info(self):
        print(f"Name: {self.name}, Age: {self.age}")
        

if __name__ == "__main__":
    name_input = input().strip()
    age_input = int(input().strip())
    person = Person(name_input, age_input)
    person.display_info()
