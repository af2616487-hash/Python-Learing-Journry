def divide_numbers():
    try:
        numerator = float(input())
        denominator = float(input())
        result = numerator / denominator
        print(result)
    except ZeroDivisionError:
        print("Error: Cannot divide by zero.")
    
    except ValueError:
        print("Error: Please enter valid numeric values.")

if __name__ == "__main__":
    divide_numbers()
